{
    "name": "smirik/propel-admin-bundle",
    "description": "Powerful propel admin generator.",
    "keywords": ["propel", "admin", "generator"],
    "homepage": "http://smirik.github.com/SmirikPropelAdminBundle/",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeny Smirnov",
            "email": "smirik@gmail.com",
            "homepage": "http://4xxi.com"
        }
    ],
    "require": {
        "symfony/symfony": ">=2.3,<2.4-dev",
        "symfony/console": ">=2.3,<2.4-dev",
        "propel/propel-bundle": "1.*",
        "willdurand/propel-typehintable-behavior": "*",
        "phpspec/phpspec": "2.0.*@dev"
    },
    "suggest": {
        "friendsofsymfony/user-bundle": "*",
        "avalanche123/imagine-bundle": "*",
        "imagine/Imagine": "*",
        "smirik/bootstrap-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Smirik\\PropelAdminBundle": "" }
    },
    "target-dir": "Smirik/PropelAdminBundle",
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable"
}
